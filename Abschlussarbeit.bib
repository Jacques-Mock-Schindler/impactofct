@article{gunckelComputationalThinkingUsing2022,
  title = {Computational {{Thinking}} for {{Using Models}} of {{Water Flow}} in {{Environmental Systems}}: {{Intertwining Three Dimensions}} in a {{Learning Progression}}},
  author = {Gunckel, Kristin L. and Covitt, Beth A. and Berkowitz, Alan R. and Caplan, Bess and Moore, John C.},
